
[sssd]
domains = corp.ad.tullib.com
config_file_version = 2
services = nss, pam

[domain/corp.ad.tullib.com]
#add the ad_server here to work around the srv records issues for nj
#ad_server = njcpinfdcg01.corp.ad.tullib.com, njcpinfdcg02.corp.ad.tullib.com
ad_domain = corp.ad.tullib.com
krb5_realm = CORP.AD.TULLIB.COM
realmd_tags = manages-system joined-with-samba
cache_credentials = True
id_provider = ad
krb5_store_password_if_offline = True
default_shell = /bin/bash
ldap_id_mapping = True
use_fully_qualified_names = False
fallback_homedir = /home/%u
access_provider = simple
simple_allow_groups = $, corp-unixsa-linux, corp-korea-neri-linux, corp-itsecurity-srvcs-linux
simple_allow_users = $